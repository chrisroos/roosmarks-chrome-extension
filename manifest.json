{
  "name": "Roosmarks",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Easy access to Roosmarks.",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://roosmarks.herokuapp.com/",
    "tabs"
  ]
}